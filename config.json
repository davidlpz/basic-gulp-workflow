{
	"wp_proxy": "localhost/folder/",
	"build": {
		"path": "../html/",
		"images": "../html/img/",
		"css": "../html/",
		"js": "../html/js/"
	},
	"src": {
		"path": ["../html/**/*.+(html|php)"],
		"images": "../html/img/",
		"css": [
			"!../html/css/**/*min.*",
			"../html/css/**/*.css"
		],
		"sass": [
			"../html/css/**/*.+(scss|sass)"
		],
		"js": [
			"!../html/js/**/*min.*",
			"../html/js/**/*.js"
		]
	}
}